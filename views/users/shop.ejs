   <script>
      $(document).ready(function () {
    // Filter button click event
    $('#filter-btn').on('click', function () {
        console.log("clicked")
        var priceRange = $('#amount').val(); // Get price range from input field
        
        // Send AJAX request to the server
        $.ajax({
            method: 'POST',
            url: '/priceFilter', // Replace with your server endpoint for filtering
            data: { priceRange: priceRange },
            success: function (response) {
                // Encode the data as a JSON string and add it as a query parameter
        var encodedData = encodeURIComponent(JSON.stringify(response.data));
        // Redirect to the filtered-products page with data as a query parameter
        window.location.href = '/filtered-products?data=' + encodedData;// Change '/filtered-products' to your desired route
            },
            error: function (error) {
                console.error('Error filtering products:', error);
                // Handle error, show a message to the user or perform other actions
            }
        });
    });
});
   </script>
