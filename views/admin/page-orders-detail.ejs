<script>
    function changeStatus(orderId){
        //alert("HIiiiiiiii")
        let status = document.getElementById('orderStatus').value
        //alert(status);
        $.ajax({
            url:'/admin/change-order-status',
            method:'post',
            data:{
                orderId,
                status,
            },
            success:(response)=>{
                alert(response);
            }
        })
    }
</script>

