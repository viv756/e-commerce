                                <option id="cancelled" value="cancelled">cancelled</option>
                                <option id="orderStatus"value="Awaiting payment">Awaiting payment</option>
                                <option id="orderStatus" value="Confirmed">Confirmed</option>
                                <option id="orderStatus" value="Shipped">Shipped</option>
                                <option id="orderStatus" value="Delivered">Delivered</option>
                            </select>
                            <a class="btn btn-primary" onclick="changeStatus('<%=data._id%>')" >Save</a>
                            <a class="btn btn-secondary print ms-2" href="#"><i class="icon material-icons md-print"></i></a>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="row mb-50 mt-20 order-info-wrap">
                        <div class="col-md-4">
